<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.lanou.Actuarialoriented.domain.CrmCourseType" table="crm_course_type" schema="crm">
        <id name="courseTypeId">
            <column name="courseTypeId" sql-type="varchar(255)"/>
            <generator class="uuid"/>
        </id>
        <property name="courseCost">
            <column name="courseCost" sql-type="double" precision="-1" />
        </property>
        <property name="total">
            <column name="total" sql-type="int(11)" />
        </property>
        <property name="courseName">
            <column name="courseName" sql-type="varchar(500)" length="500" />
        </property>
        <property name="remark">
            <column name="remark" sql-type="varchar(5000)" length="5000" />
        </property>

    </class>



</hibernate-mapping>